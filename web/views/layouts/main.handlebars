<html>
  <title>{{ title }} - {{ company }}</title>
  <head>
   <!-- Google tag (gtag.js) -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=G-78ZJXQQS7Y"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag(){dataLayer.push(arguments);}
        gtag('js', new Date());

        gtag('config', 'G-78ZJXQQS7Y');
    </script>
    <!-- favicon -->
    <link rel="shortcut icon" href="/web/images/favicon.ico" type="image/x-icon" />

    <link href='/web/css/styles.css' rel='stylesheet' type='text/css'>

    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600;700&display=swap" rel="stylesheet">
    <link href='https://fonts.googleapis.com/css?family=Khand' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Open+Sans:400,500,600,700' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Roboto+Condensed' rel='stylesheet' type='text/css'>
    
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

    <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chart.js@4.3.0/dist/chart.umd.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-annotation@3.0.1/dist/chartjs-plugin-annotation.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels@2.2.0/dist/chartjs-plugin-datalabels.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/jquery@3/dist/jquery.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/js-cookie@rc/dist/js.cookie.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/lodash@4/lodash.min.js"></script>
    <script src="/web/js/chart.js"></script>
    <script src="/web/js/content.js"></script>
    <!--<script src="/web/js/chartjs-plugin-watermark.min.js"></script>-->

    <meta property='og:title' content='{{ title }}' />
    <meta id='metaURL' property='og:url' content='https://benchmark.bespoken.io' />      
    <meta property="og:description" content="{{ description }}" />
    <meta property="og:image" content="/web/images/MainGraph.png" />
    <meta property="og:type" content="article" />


    <script>
      // We set certain style programmatically because they are shared between charts generated in JS and HTML
      window.addEventListener('load', () => {
        // Automatically add href to every a anchor
        $('a[name]').each(function() {
          this.href = '#' + this.name
        })

        $('#metaURL').attr('content', window. location. href)
      })

      function showInfo(key) {
        const title = Content.titleForKey(key)
        const content = Content.contentForKey(key)
        $('#modal-title').html(title)
        $('#modal-body').html(content)
        $('#infoModal').modal({})   
      }          
    </script>
  </head>
  <body style='margin-bottom: 10px'>
    <div class='container-fluid ' style='background-color: #184160;padding: 10;'>
      <div class='row header'>
        <div class='col-4' style='text-align: left;display:inline-flex;margin-bottom: auto;margin-top: auto'><a href='https://bespoken.io'><img src='/web/images/Logo-WhiteText.png' class='logo' /></a></div>
        <div class='col-4'>

          <div class='row' style="height:100%">
            <!-- got this display and justify from here - https://stackoverflow.com/a/50598356 -  to force centering without wrapping -->
            <div class='col-4' style='padding:0;display: flex;justify-content: center;white-space: nowrap;align-items: center'>
                <h1 style='font-family: Poppins;margin:0;'><a href='/' style="text-decoration: none;color: white">{{title}}</a></h1>
            </div> 
          </div>
          {{#if page}}
            <div class='row' >
                <div class='col-4' style='padding:0;display: flex;justify-content: center;white-space: nowrap'><h2 style='color: #1ec9dd;font-weight: bold;margin:0;text-align: center;'> {{page}}</h2></div> 
            </div>
          {{/if}}
        </div>
        <div class='col-4  d-flex align-items-center justify-content-end' id='sponsorLogoSection' style='display:inline-flex;text-align: right;'>
          {{#if sponsorLogo}}<a href='{{sponsorURL}}' target='_blank' ><img class='sponsorLogo' src='/web/images/{{sponsorLogo}}' style='padding: 0' /></a>{{/if}}
        </div>
        
      </div>  
      
    </div>
    <div class='container-fluid menu' style='margin-bottom: 10px'>
      <div class='row' >
        <div class='col-4 menu' style='padding:3;display: flex;justify-content: center;white-space: nowrap'><span class='menu-title'>Our Benchmarks:&nbsp; </span>&nbsp;
           <span class='menu-options'><a href='/ivr'>IVR</a> &nbsp; | &nbsp; <a href='/llm'>LLM</a> &nbsp; | &nbsp; <a href='/nlp'>Virtual Assistant</a> </span></div> 
      </div>
    </div>
    {{{body}}}

    <!-- footer -->
    <div class='container-fluid footer'>
      <div class='row'>
        <div class='col-12' style='font-style: italic'>Copyright Â© 2023 {{ company }}. All Rights Reserved</div>
      </div>
    </div>
    <!-- for showing popup informational dialog on the page -->
    <div id='infoModal' class="modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true" style='margin-top: 100px'>
      <div class="modal-dialog" role="document">
        <div class="modal-content">
          <div class="modal-header" style='padding-bottom: 0.5em;'>
            <h5 class="title modal-title" id="modal-title" >Modal title</h5>
          </div>
          <div id='modal-body' class="text modal-body" style='padding-top: 0.5em;text-align: left'>
            
          </div>
        </div>
      </div>
    </div>
    
  </body>
</html>