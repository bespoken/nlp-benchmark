    "reprocess:alexa": "bbt reprocess input/nlp-benchmark-alexa.json",
    "reprocess:google": "bbt reprocess input/nlp-benchmark-google.json",
    "reprocess:siri": "bbt reprocess input/nlp-benchmark-siri.json",
